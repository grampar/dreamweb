/* 캘린더 */
#calendar1 {position:relative;background:url(http://img.noribox.com/b2b/netmarble/img/chart/calendar_bg1.gif);width:212px;height:160px;}
#calendar1 #top{position:relative;width:210px;height:31px;top:10px;left:0; text-align:center;}
#calendar1 #top img{ vertical-align:middle; border:0;}
#calendar1 #top span{color:#fff;font-weight:bold;font-size:14px;width:100px;padding:0 5px 0 5px;display:inline-block}
#calendar1 #close{position:absolute;top:7px;left:196px;}
#calendar1 #close img{border:0;}

/* 캘린더_일간 */
#calendar1 #day{position:relative;width:208px;top:0;left:1px;background:url(http://img.noribox.com/b2b/netmarble/img/chart/calendar_day_bg.gif) repeat-x; border:}
#calendar1 #day table {position:relative;top:2px; margin:0 auto;}
#calendar1 #day table th{width:27px;height:20px;color:#666; font-weight:100; font-family:돋움;font-size:11px;text-align:center;padding:3px 0 3px 0;}
#calendar1 #day table th.w1{color:#FF5A00}
#calendar1 #day table td{width:27px;height:16px;color:#666;font-family:Tahoma;font-size:10px;text-align:center; display:block;} 
#calendar1 #day table td.sunday{color:#FF5A00}
#calendar1 #day table td.today{color:#000;font-weight:bold;}
#calendar1 #day table td.after{color:#aaa;}
#calendar1 #day table td.hit{color:#FF5A00;background:#eee;font-weight:bold;}

/* 출시 준비 게임 */
#calendar { position:relative; width:100%}
#today { font-family:dotum,Tahoma; letter-spacing:-1px; position:relative; width:195px; height:16px; font-size:11px;}
#today.m1, #today.m1 a {color:#267ed0; }
#today.m2, #today.m2 a {color:#8a26d0; }
#today.m3, #today.m3 a {color:#e44b88; }
#today.m4, #today.m4 a {color:#08a121; }
#today.m5, #today.m5 a {color:#f48207; }
#today.m6, #today.m6 a {color:#cd283c; }
#today.m7, #today.m7 a {color:#5d5d5d; }
#today.m8, #today.m8 a {color:#1fadaf; }
#today.m9, #today.m9 a {color:#116140; }
#today span { position:relative; vertical-align:top; margin:3px; top:1px; line-height:15px;}

/* UI Object */ 

.tab_face{padding:0 0 15px 0}  
.tab_face ul{position:relative;margin:0;padding:0;_padding:2px 0 0 0;border-bottom:2px solid #396ab5;font-family:Tahoma, Sans-serif;font-size:12px;list-style:none;*zoom:1}  
.tab_face ul:after{display:block;clear:both;content:""}  
.tab_face li{float:left;margin-right:1px;background:#4cb1e5;}  
.tab_face li a,  
.tab_face li a span{position:relative;cursor:pointer}  
.tab_face li a{float:left;text-decoration:none}
.tab_face li a:hover { text-decoration:none}
.tab_face li a span{display:inline-block;padding:6px 25px 6px 25px;color:#fff}  
.tab_face li ul{display:none;overflow:hidden;position:absolute;top:40px;left:0;width:100%;margin:0;padding:0;border:0;white-space:nowrap;list-style:none;*zoom:1}  
.tab_face li ul:after{display:block;clear:both;content:""}  
.tab_face li li{overflow:visible;position:relative;left:-1px;padding:0 10px;border-left:1px solid #ddd;background:transparent}  
.tab_face li li a{padding:0;margin:0;font-weight:normal;color:#666}  
.tab_face.m1 .m1,  
.tab_face.m2 .m2,  
.tab_face.m3 .m3,  
.tab_face.m4 .m4,  
.tab_face.m5 .m5,  
.tab_face.m6 .m6,  
.tab_face.m7 .m7,  
.tab_face.m8 .m8,  
.tab_face.m9 .m9,  
.tab_face.m10 .m10,  
.tab_face.m11 .m11,  
.tab_face.m12 .m12{margin-top:-1px;background-color:#396ab5;}  
.tab_face.m1 .m1 a span,  
.tab_face.m2 .m2 a span,  
.tab_face.m3 .m3 a span,  
.tab_face.m4 .m4 a span,  
.tab_face.m5 .m5 a span,  
.tab_face.m6 .m6 a span,  
.tab_face.m7 .m7 a span,  
.tab_face.m8 .m8 a span,  
.tab_face.m9 .m9 a span,  
.tab_face.m10 .m10 a span,  
.tab_face.m11 .m11 a span,  
.tab_face.m12 .m12 a span{padding-top:7px;padding-bottom:7px;font-weight:bold;letter-spacing:-1px}  
.tab_face.m1 .m1 ul,  
.tab_face.m2 .m2 ul,  
.tab_face.m3 .m3 ul,  
.tab_face.m4 .m4 ul,  
.tab_face.m5 .m5 ul,  
.tab_face.m6 .m6 ul,  
.tab_face.m7 .m7 ul,  
.tab_face.m8 .m8 ul,  
.tab_face.m9 .m9 ul,  
.tab_face.m10 .m10 ul,  
.tab_face.m11 .m11 ul,  
.tab_face.m12 .m12 ul{display:block}  
.tab_face li li a:hover,  
.tab_face li li a:active,  
.tab_face li li a:focus,  
.tab_face.s1 .s1 a,  
.tab_face.s2 .s2 a,  
.tab_face.s3 .s3 a,  
.tab_face.s4 .s4 a,  
.tab_face.s5 .s5 a,  
.tab_face.s6 .s6 a,  
.tab_face.s7 .s7 a,  
.tab_face.s8 .s8 a,  
.tab_face.s9 .s9 a,  
.tab_face.s10 .s10 a,  
.tab_face.s11 .s11 a,  
.tab_face.s12 .s12 a{font-weight:bold;color:#333;letter-spacing:-1px}  
.tab_face.js_off{padding:0}  
.tab_face.js_off ul{border:0}  
.tab_face.js_off li{float:none}  
.tab_face.js_off li a{float:none;font-weight:bold}  
.tab_face.js_off li ul{display:block;position:static;left:0;top:0;padding:5px 0;background:#fff}  
.tab_face.js_off li li{display:inline}  
.tab_face.js_off li li a{font-weight:normal !important;color:#333;letter-spacing:normal}  
.tab_face.js_off li li a:hover,  
.tab_face.js_off li li a:active,  
.tab_face.js_off li li a:focus{text-decoration:underline}  

