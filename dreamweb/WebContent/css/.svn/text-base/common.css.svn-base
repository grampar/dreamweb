/* 진행상태 */
.progress{height:25px;margin:10px 0 0;background:url(../img/bg_progress.gif);border-right:1px solid #e3e3e3;font-family:'돋움',dotum;font-size:11px;line-height:15px;letter-spacing:-1px}  
.progress .tit{float:left;position:relative;z-index:1000;width:74px;height:100%;margin:0;padding:5px 0 0;background:url(../img/bg_progress_tit.gif) y-repeat;font-size:11px;color:#fff;line-height:15px;text-align:center}  
.progress ol{float:left;margin:0;padding:0;list-style:none;}  
.progress li,.progress li span{background:url(../img/bg_progress_item.gif) no-repeat;font-size:11px;white-space:nowrap;}  
.progress li{float:left;margin:0 0 0 -13px;padding:0 0 0 13px;background-position:-287px 0}  
.progress li span{display:inline-block;height:25px;padding:0 15px 0 13px;background-position:100% top;line-height:25px;}  
.progress li.on{position:relative;z-index:10;background-position:0 100%;color:#fff}
.progress li.on a{color:#fff}
.progress li.on span{background:url(../img/bg_progress_item.gif) no-repeat 100% 100%}  
.progress li.last,.progress li.last span{background:none}  
.progress .right{position:absolute;top:6px;right:10px;color:#444}

